<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruanmou.house.server.user.mapper.UserMapper">
    <update id="updateUser" parameterType="com.ruanmou.house.server.user.domain.User">
        update `user` set enable = #{enable} where email = #{email}
    </update>
</mapper>